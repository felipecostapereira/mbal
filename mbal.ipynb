    "import openserver\n",
    "import os\n",
    "\n",
    "petex = openserver.OpenServer()\n",
    "\n",
    "with petex:\n",
    "    petex.DoCmd('MBAL.START')\n",
    "    path = os.path.join(os.getcwd(),'monai_p50_blackoil.mbi')\n",
    "    petex.DoCmd(f'MBAL.OPENFILE(\"{path}\")')\n",
    "    petex.DoCmd('MBAL.SHUTDOWN')"
   ]
  }
